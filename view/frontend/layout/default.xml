<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">	
	<head>
		<css src="Ced_Booking::css/font-awesome.min.css" />
		<css src="Ced_Booking::css/font-awesome.css" />
		<css src="Ced_Booking::css/booking-custom.css" />
	</head>
	<body>      
		<referenceBlock name="header.links">
			<block class="Ced\Booking\Block\Links" name="booking_menu" template="nav/links.phtml"/>
		</referenceBlock>
		<referenceContainer name="sidebar.main">
            <block class="Ced\Booking\Block\AdditionalFilter" name="booking.additional.filter" template="additional_filters.phtml" after="catalog.compare.sidebar"/>
        </referenceContainer>
	</body>
</page>
